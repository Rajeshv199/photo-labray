<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("common/header") -%>

    <title>Photo Labrary</title>
</head>


<body>

    <!-- navber start -->
    <%- include("common/navber") -%>

 

    <!-- end  -->

    <div class="head-container">

        <div class="show-title">
            <h1>Free library</h1>
        </div>

        <!-- Category start  -->
        <div class="head-title">

            <div class="title"><a href="">Book</a></div>
            <div class="title"><a href="">Library</a></div>
            <div class="title"><a href="">Room</a></div>
            <div class="title"><a href="">Reading</a></div>
            <div class="title"><a href="">Nature</a></div>
            <div class="title"><a href="">Indoor</a></div>
            <div class="title"><a href="">Study</a></div>

        </div>

        <!-- end  -->



        <!-- show img start  -->
        <div class="head-image ">
            <% items.forEach(function(image) { %>
                <div class="imgg">
                    <a href="imgshow"><img src="data:image/<%=image.img.contentType%>;base64,
                    <%=image.img.data.toString('base64')%>"></a>
                </div>
                <% }) %>
        </div>

        <!-- end -->

    </div>

    <!-- footer page -->
    <%- include("common/footer") -%>
    
    <!-- end  -->



</body>

</html>